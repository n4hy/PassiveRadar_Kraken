    run_command: '{python} -u {filename}'
    run_options: prompt
    sizing_mode: fixed
    thread_safe_setters: ''
    title: KrakenSDR 5-CH Monitor
    window_size: ''
  states:
    coordinate:
    - 8
    - 8
    rotation: 0
    state: enabled
metadata:
  file_format: 1
blocks:
- id: variable
  parameters:
    comment: ''
    id: samp_rate
    value: '2048000'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 200
    - 12
    rotation: 0
    state: enabled
  name: samp_rate
- id: variable
  parameters:
    comment: ''
    id: center_freq
    value: 99.5e6
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 300
    - 12
    rotation: 0
    state: enabled
  name: center_freq
- id: variable
  parameters:
    comment: ''
    id: rf_gain
    value: '10'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 400
    - 12
    rotation: 0
    state: enabled
  name: rf_gain

# Source (Using the new Custom Block)
- id: kraken_passive_radar_krakensdr_source
  parameters:
