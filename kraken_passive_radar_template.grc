options:
  parameters:
    author: ChatGPT
    category: Custom
    cmake_opt: ''
    comment: ''
    copyright: ''
    description: Two-channel passive radar front-end with FFT cross-correlation and
      range-time raster.
    gen_cmake: 'On'
    gen_linking: dynamic
    generate_options: qt_gui
    hier_block_src_path: '.:'
    id: top_block
    max_nouts: '0'
    output_language: python
    placement: (0,0)
    qt_qss_theme: ''
    realtime_scheduling: ''
    run: 'True'
    run_command: '{python} -u {filename}'
    run_options: prompt
    'sizing_mode:': fixed
    thread_safe_setters: ''
    title: KrakenSDR Passive Radar (FM/TV Illuminator)
    window_size: 1280,800
  states:
    coordinate:
    - 8
    - 8
    rotation: 0
    state: enabled
blocks:
- id: variable
  parameters:
    id: samp_rate
    value: '2048000'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 8
    - 150
    rotation: 0
    state: enabled
  name: samp_rate
- id: variable
  parameters:
    id: center_freq
    value: 99.5e6
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 208
    - 150
    rotation: 0
    state: enabled
  name: center_freq
- id: variable
  parameters:
    id: gain
    value: '30'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 308
    - 150
    rotation: 0
    state: enabled
  name: gain
- id: variable
  parameters:
    id: fft_len
    value: '4096'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
