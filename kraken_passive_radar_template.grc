  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 208
    - 250
    rotation: 0
    state: enabled
  name: bpf_bw
- id: kraken_passive_radar_krakensdr_source
  parameters:
    frequency: center_freq
    gain: gain
    sample_rate: samp_rate
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate:
    - 408
    - 250
    rotation: 0
    state: enabled
  name: krakensdr_source_0
- id: freq_xlating_fir_filter_ccc
  parameters:
    id: ref_chan
    decim: decim
    taps: firdes.low_pass(1.0, samp_rate, bpf_bw/2, bpf_bw/4, firdes.WIN_HAMMING)
