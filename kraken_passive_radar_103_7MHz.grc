options:
  parameters:
    author: N4HY
    catch_exceptions: 'True'
    category: '[GRC Hier Blocks]'
    cmake_opt: ''
    comment: 'KrakenSDR Passive Radar - 103.7 MHz FM Illuminator'
    copyright: ''
    description: 'Passive Bistatic Radar using FM broadcast as illuminator of opportunity'
    gen_cmake: 'On'
    gen_linking: dynamic
    generate_options: qt_gui
    hier_block_src_path: '.:'
    id: kraken_passive_radar
    max_nouts: '0'
    output_language: python
    placement: (0,0)
    qt_qss_theme: ''
    realtime_scheduling: ''
    run: 'True'
    run_command: '{python} -u {filename}'
    run_options: prompt
    sizing_mode: fixed
    thread_safe_setters: ''
    title: 'KrakenSDR Passive Radar'
    window_size: (1920,1080)
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 8]
    rotation: 0
    state: enabled

blocks:
# ============================================================================
# VARIABLES
# ============================================================================
- name: samp_rate
  id: variable
  parameters:
    comment: 'KrakenSDR native sample rate'
    value: '2400000'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 160]
    rotation: 0
    state: enabled

- name: center_freq
  id: variable
  parameters:
    comment: 'FM station frequency'
    value: '103.7e6'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 240]
    rotation: 0
    state: enabled

- name: signal_bw
  id: variable
  parameters:
    comment: 'Signal bandwidth of interest'
    value: '250000'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 320]
    rotation: 0
    state: enabled

- name: decimation
  id: variable
  parameters:
    comment: 'Decimation factor'
    value: 'int(samp_rate / signal_bw)'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 400]
    rotation: 0
    state: enabled

- name: decimated_rate
  id: variable
  parameters:
    comment: 'Post-decimation sample rate'
    value: 'samp_rate / decimation'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 480]
    rotation: 0
    state: enabled

- name: cpi_samples
  id: variable
  parameters:
    comment: 'Coherent Processing Interval in samples (~100ms)'
    value: 'int(decimated_rate * 0.1)'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 560]
    rotation: 0
    state: enabled

- name: eca_taps
  id: variable
  parameters:
    comment: 'ECA filter taps (clutter delay spread)'
    value: '128'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 640]
    rotation: 0
    state: enabled

- name: eca_reg
  id: variable
  parameters:
    comment: 'ECA regularization factor'
    value: '0.001'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [8, 720]
    rotation: 0
    state: enabled

- name: num_range_bins
  id: variable
  parameters:
    comment: 'Range bins for CAF'
    value: '512'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [120, 160]
    rotation: 0
    state: enabled

- name: num_doppler_bins
  id: variable
  parameters:
    comment: 'Doppler bins for CAF'
    value: '256'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [120, 240]
    rotation: 0
    state: enabled

- name: lpf_taps
  id: variable
  parameters:
    comment: 'Low-pass filter taps for decimation'
    value: 'firdes.low_pass(1.0, samp_rate, signal_bw/2, signal_bw/10, window.WIN_HAMMING)'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [120, 320]
    rotation: 0
    state: enabled

