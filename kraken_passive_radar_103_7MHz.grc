# ============================================================================
# CONNECTIONS
# ============================================================================
connections:
# Source to DC blockers
- [soapy_krakensdr_source, '0', dc_blocker_ref, '0']
- [soapy_krakensdr_source, '1', dc_blocker_surv0, '0']
- [soapy_krakensdr_source, '2', dc_blocker_surv1, '0']
- [soapy_krakensdr_source, '3', dc_blocker_surv2, '0']
- [soapy_krakensdr_source, '4', dc_blocker_surv3, '0']

# DC blockers to freq xlating filters
- [dc_blocker_ref, '0', freq_xlating_fir_ref, '0']
- [dc_blocker_surv0, '0', freq_xlating_fir_surv0, '0']
- [dc_blocker_surv1, '0', freq_xlating_fir_surv1, '0']
- [dc_blocker_surv2, '0', freq_xlating_fir_surv2, '0']
- [dc_blocker_surv3, '0', freq_xlating_fir_surv3, '0']

# Freq xlating to AGC
- [freq_xlating_fir_ref, '0', agc_ref, '0']
- [freq_xlating_fir_surv0, '0', agc_surv0, '0']
- [freq_xlating_fir_surv1, '0', agc_surv1, '0']
- [freq_xlating_fir_surv2, '0', agc_surv2, '0']
- [freq_xlating_fir_surv3, '0', agc_surv3, '0']

# Reference AGC to ECA (input 0) and to stream-to-vector for CAF
- [agc_ref, '0', eca_canceller, '0']
- [agc_ref, '0', s2v_ref, '0']

# Surveillance AGC to ECA (inputs 1-4)
- [agc_surv0, '0', eca_canceller, '1']
- [agc_surv1, '0', eca_canceller, '2']
- [agc_surv2, '0', eca_canceller, '3']
- [agc_surv3, '0', eca_canceller, '4']

# ECA outputs to stream-to-vector
- [eca_canceller, '0', s2v_surv0, '0']
- [eca_canceller, '1', s2v_surv1, '0']
- [eca_canceller, '2', s2v_surv2, '0']
- [eca_canceller, '3', s2v_surv3, '0']

# Stream-to-vector to FFTs
- [s2v_ref, '0', fft_ref, '0']
- [s2v_surv0, '0', fft_surv0, '0']
- [s2v_surv1, '0', fft_surv1, '0']
- [s2v_surv2, '0', fft_surv2, '0']
- [s2v_surv3, '0', fft_surv3, '0']

# Reference FFT to conjugate
- [fft_ref, '0', conjugate_ref, '0']

# Surveillance FFTs + conjugate ref to multiply
- [fft_surv0, '0', multiply_conj_surv0, '0']
- [conjugate_ref, '0', multiply_conj_surv0, '1']
- [fft_surv1, '0', multiply_conj_surv1, '0']
- [conjugate_ref, '0', multiply_conj_surv1, '1']
- [fft_surv2, '0', multiply_conj_surv2, '0']
- [conjugate_ref, '0', multiply_conj_surv2, '1']
- [fft_surv3, '0', multiply_conj_surv3, '0']
- [conjugate_ref, '0', multiply_conj_surv3, '1']

# Cross-spectrum to IFFT
- [multiply_conj_surv0, '0', ifft_surv0, '0']
- [multiply_conj_surv1, '0', ifft_surv1, '0']
- [multiply_conj_surv2, '0', ifft_surv2, '0']
- [multiply_conj_surv3, '0', ifft_surv3, '0']

# IFFT to magnitude squared
- [ifft_surv0, '0', mag_squared_surv0, '0']
- [ifft_surv1, '0', mag_squared_surv1, '0']
- [ifft_surv2, '0', mag_squared_surv2, '0']
- [ifft_surv3, '0', mag_squared_surv3, '0']

# Magnitude squared to fusion adder
- [mag_squared_surv0, '0', add_fusion, '0']
- [mag_squared_surv1, '0', add_fusion, '1']
- [mag_squared_surv2, '0', add_fusion, '2']
- [mag_squared_surv3, '0', add_fusion, '3']

# Fusion to log scale
- [add_fusion, '0', nlog10, '0']

# Log scale to display and file
- [nlog10, '0', qtgui_vector_sink, '0']
- [nlog10, '0', file_sink_rd_map, '0']

metadata:
  file_format: 1
  grc_version: 3.10.0.0
